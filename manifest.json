{
    "manifest_version": 3,
    "name": "Samsung OSS Helper",
    "version": "2.0",
    "description": "Helper extension to capture download requests from Samsung Open Source",
    "permissions": [
        "webRequest",
        "cookies",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://opensource.samsung.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}
